{
    "configurations": [
        {
            "name": "ESP-IDF",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "forcedInclude": [
                "${HOME}/esp/esp-idf/components/mqtt/host_test/mocks/include/freertos/portmacro.h",
                "${HOME}/esp/esp-idf/components/freertos/tasks.c",
                "${HOME}/esp/esp-idf/components/freertos/**"
            ],
            "includePath": [
                "${config:idf.espIdfPath}/components/**",
                "${config:idf.espIdfPathWin}/components/**",
